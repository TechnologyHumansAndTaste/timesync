{
  "name": "timesync",
  "version": "1.0.7",
  "description": "Time synchronization between peers",
  "author": "Jos de Jong <wjosdejong@gmail.com> (https://github.com/josdejong)",
  "main": "./lib/timesync.js",
  "license": "MIT",
  "keywords": [
    "time",
    "synchronization",
    "ntp",
    "client",
    "server",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/enmasseio/timesync.git"
  },
  "dependencies": {
    "debug": "4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "babel-cli": "6.26.0",
    "babelify": "10.0.0",
    "body-parser": "1.19.0",
    "browserify": "16.5.1",
    "express": "4.17.1",
    "mkdirp": "1.0.4",
    "promise": "8.1.0",
    "socket.io": "2.3.0",
    "uglify-js": "3.10.0",
    "watch": "1.0.2"
  },
  "scripts": {
    "bundle": "mkdirp dist && browserify src/timesync.js -t babelify -s timesync -o dist/timesync.js --bare",
    "minify": "uglifyjs dist/timesync.js -o dist/timesync.min.js",
    "compile": "babel src --out-dir lib",
    "build": "npm run bundle && npm run minify && npm run compile",
    "watch": "watch \"npm run build\" src",
    "prepublishOnly": "npm run build"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "files": [
    "dist",
    "docs",
    "examples",
    "lib",
    "server",
    "src",
    "HISTORY.md",
    "LICENSE",
    "README.md"
  ]
}
